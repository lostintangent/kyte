{
  "name": "kyte",
  "version": "0.2.4",
  "description":
    "A single-command CLI for sharing and collaboratively editing a file",
  "repository": "https://github.com/lostintangent/kyte",
  "bin": {
    "kyte": "bin/index.js"
  },
  "files": ["bin/", "lib/", "server/index.js", "server/dist/"],
  "scripts": {
    "build":
      "parcel build server/client/index.html -d server/dist --public-url '/' --no-minify",
    "postpublish": "rm ./README.md",
    "prepublishOnly": "cp ../../README.md ./README.md"
  },
  "author": "Jonathan Carter",
  "license": "MIT",
  "dependencies": {
    "@babel/polyfill": "^7.0.0-beta.34",
    "boxen": "^1.2.1",
    "chalk": "^2.1.0",
    "copy-paste": "^1.3.0",
    "express": "^4.15.4",
    "get-port": "^3.2.0",
    "monaco-share": "^0.2.2",
    "ngrok": "^2.2.22",
    "node-emoji": "^1.8.1",
    "node-version": "^1.1.0",
    "opn": "^5.1.0",
    "ot-text": "^1.0.1",
    "sharedb": "^1.0.0-beta.7",
    "websocket-json-stream": "^0.0.3",
    "ws": "^3.2.0",
    "yargs": "^10.0.3"
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
